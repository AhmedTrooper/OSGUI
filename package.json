{
  "name": "osgui",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "Modern GUI application for yt-dlp commandline downloader with enhanced TypeScript, React, and Tauri v2",
  "author": "AhmedTrooper",
  "license": "MIT",
  "homepage": "https://github.com/AhmedTrooper/OSGUI",
  "repository": {
    "type": "git",
    "url": "https://github.com/AhmedTrooper/OSGUI.git"
  },
  "keywords": [
    "tauri",
    "react",
    "typescript",
    "yt-dlp",
    "desktop",
    "downloader",
    "gui"
  ],
  "scripts": {
    "dev": "vite --host",
    "dev:debug": "vite --host --mode development --debug",
    "build": "tsc && vite build",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:build:debug": "tauri build --debug",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "format:check": "prettier --check src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "clean": "rimraf dist src-tauri/target",
    "clean:all": "rimraf dist src-tauri/target node_modules",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "deps:check": "npm-check-updates",
    "deps:update": "npm-check-updates -u && npm install",
    "bundle:analyze": "npx vite-bundle-analyzer dist"
  },
  "dependencies": {
    "@fontsource-variable/inter": "^5.2.5",
    "@heroui/alert": "^2.2.20",
    "@heroui/card": "^2.2.19",
    "@heroui/code": "^2.2.15",
    "@heroui/react": "^2.7.9",
    "@react-icons/all-files": "^4.1.0",
    "@supabase/supabase-js": "^2.57.4",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-clipboard-manager": "^2.3.0",
    "@tauri-apps/plugin-fs": "^2.3.0",
    "@tauri-apps/plugin-notification": "^2.2.3",
    "@tauri-apps/plugin-opener": "^2.3.0",
    "@tauri-apps/plugin-os": "^2.3.1",
    "@tauri-apps/plugin-shell": "^2.2.2",
    "@tauri-apps/plugin-sql": "^2.2.1",
    "@tauri-apps/plugin-store": "^2.2.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.16.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.513.0",
    "nanoid": "^5.1.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.2",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2",
    "@types/lodash": "^4.17.17",
    "@types/node": "^22.15.30",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^2.1.8",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "npm-check-updates": "^17.1.11",
    "postcss": "^8.5.4",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.2",
    "vite": "^6.0.3",
    "vite-bundle-analyzer": "^0.18.0",
    "vitest": "^2.1.8"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
